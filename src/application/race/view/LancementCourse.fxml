<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Group?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>

<AnchorPane xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.race.controller.LaunchController">
   <children>
      <Group fx:id="groupStart" layoutX="10.0" layoutY="10.0">
         <children>
            <Pane fx:id="configStart" layoutX="-12.0" layoutY="-12.0" prefHeight="298.0" prefWidth="252.0">
               <children>
                  <TextField fx:id="vitesseV1" layoutX="16.0" layoutY="74.0" prefHeight="10.0" prefWidth="105.0" promptText="Vitesse voiture 1" />
                  <TextField fx:id="vitesseV2" layoutX="128.0" layoutY="74.0" prefHeight="10.0" prefWidth="105.0" promptText="Vitesse voiture 2" />
                  <TextField fx:id="vitesseV3" layoutX="16.0" layoutY="110.0" prefHeight="10.0" prefWidth="105.0" promptText="Vitesse voiture 3" />
                  <TextField fx:id="vitesseV4" layoutX="128.0" layoutY="110.0" prefHeight="10.0" prefWidth="105.0" promptText="Vitesse voiture 4" />
                  <TextField fx:id="vitesseV5" layoutX="68.0" layoutY="149.0" prefHeight="10.0" prefWidth="105.0" promptText="Vitesse voiture 5" />
                  <Button fx:id="start" layoutX="98.0" layoutY="196.0" mnemonicParsing="false" onAction="#startCourse" prefHeight="10.0" prefWidth="60.0" text="Start" />
                  <Label fx:id="erreur" layoutX="34.0" layoutY="248.0" prefHeight="36.0" prefWidth="189.0" textFill="RED" wrapText="true" />
                  <Label layoutX="45.0" layoutY="31.0" text="Animation Course de voiture" />
                  <Button fx:id="retour" layoutX="186.0" layoutY="259.0" mnemonicParsing="false" onAction="#clic" text="Retour" />
               </children>
            </Pane>
         </children>
      </Group>
   </children>
</AnchorPane>
